{
  "all": true,
  "check-coverage": true,
  "lines": 70,
  "functions": 70,
  "branches": 70,
  "statements": 70,
  "include": ["src/**/*.ts"],
  "exclude": ["src/**/*.d.ts", "test/**/*", "node_modules/**"],
  "extension": [".ts"],
  "require": ["ts-node/register"],
  "reporter": ["text", "html", "lcov"],
  "report-dir": "./coverage",
  "sourceMap": true,
  "instrument": true
}
