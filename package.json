{
  "name": "discord-qa-bot",
  "version": "1.0.0",
  "description": "Discord Q&A Bot with AI-powered knowledge base",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "mocha",
    "test:watch": "mocha --watch",
    "test:coverage": "nyc mocha",
    "lint": "eslint src test --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "keywords": [
    "discord",
    "bot",
    "qa",
    "ai",
    "gemini"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.6",
    "@types/sinon": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "chai": "^4.3.10",
    "eslint": "^8.56.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^3.1.1",
    "sinon": "^17.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@google-cloud/firestore": "^7.1.0",
    "@google/generative-ai": "^0.1.3",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "pdf-parse": "^1.1.1"
  }
